 <div class="container">
        <table class="table table-striped table-dark m-2">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Item</th>
      <th scope="col">Brand</th>
      <th scope="col">model no</th>
      <th scope="col">stock</th>
    </tr>
  </thead>
  <tbody>

    {{#each stocks}}
    <tr>
      <th scope="row">{{ @index}}</th>
      <td>{{this.item}}</td>
      <td>{{this.brand}}</td>
      <td>{{this.modelNo}}</td>
      <td>{{this.stock}}</td>
    </tr>
    {{/each}}

  </tbody>
</table>
 </div>